{
  "name": "simple-transcoding",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "TS_NODE_FILES=true ava",
    "pretest": "tslint -p .",
    "build": "tsc -p .; node src/build.js; cp -r src/bin/ build/bin/",
    "prepare": "npm run build",
    "watch": "tsc --watch -p .",
    "start": "node build/server.js",
    "dev": "node src/build.js; ts-node-dev --respawn src/server.ts"
  },
  "author": "Dwira Maulana",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.0.1",
    "fp-ts": "^2.5.3",
    "lodash": "^4.17.21",
    "shaka-packager": "^2.4.1",
    "shell-escape": "^0.2.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.3",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/lodash": "^4.14.149",
    "@types/shell-escape": "^0.2.0",
    "ava": "^3.5.1",
    "axios-mock-adapter": "^1.18.1",
    "mkdirp": "^1.0.3",
    "node-wget-promise": "^0.1.6",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.0",
    "tslint-config-standard": "^9.0.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**/*.ts"
    ]
  }
}
